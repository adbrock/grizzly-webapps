<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>


<AnchorPane prefHeight="400.0" prefWidth="400" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<VBox spacing="10.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
			<children>
				<HBox spacing="8.0" AnchorPane.bottomAnchor="0" AnchorPane.leftAnchor="0" AnchorPane.rightAnchor="0" AnchorPane.topAnchor="0">
					<children>
						<Button onAction="#handleRefreshAction" mnemonicParsing="false" text="Refresh" />
						<Label alignment="CENTER_RIGHT" maxWidth="1.7976931348623157E308" prefHeight="25.0" text="Listening on port:" HBox.hgrow="ALWAYS" />
						<ChoiceBox fx:id="cboPort" />
					</children>
				</HBox>
				<Separator prefWidth="200.0" />
				<HBox spacing="8.0" AnchorPane.bottomAnchor="0" AnchorPane.leftAnchor="0" AnchorPane.rightAnchor="0" AnchorPane.topAnchor="0" VBox.vgrow="ALWAYS">
					<children>
						<VBox>
							<children>
								<Label alignment="CENTER" maxWidth="1.7976931348623157E308" text="Active" />
								<ListView fx:id="lstActive" AnchorPane.bottomAnchor="0" AnchorPane.leftAnchor="0" AnchorPane.rightAnchor="0" AnchorPane.topAnchor="0" />
							</children>
						</VBox>
						<VBox>
							<children>
								<Label alignment="CENTER" maxWidth="1.7976931348623157E308" text="Available" />
								<ListView fx:id="lstAvailable" AnchorPane.bottomAnchor="0" AnchorPane.leftAnchor="0" AnchorPane.rightAnchor="0" AnchorPane.topAnchor="0" />
							</children>
						</VBox>
					</children>
				</HBox>
				<HBox spacing="8.0">
					<children>
						<TextField fx:id="txtId" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS" />
						<Button fx:id="btnAdd" onAction="#handleAddAction" mnemonicParsing="false" text="Add" />
						<Button onAction="#handleRemoveAction" mnemonicParsing="false" text="Remove" />
					</children>
				</HBox>
				<Separator prefWidth="200.0" />
				<ListView fx:id="lstProblems" minHeight="60.0" prefHeight="60.0" />
			</children>
		</VBox>
	</children>
</AnchorPane>
